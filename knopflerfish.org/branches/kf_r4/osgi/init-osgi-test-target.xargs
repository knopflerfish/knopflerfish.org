#
# Startup file for executing the OSGi RI test suite
#

-Dapple.laf.useScreenMenuBar=true

#
# Act as an OSGi test director target
#
-Dorg.osgi.test.target.port=3191
-Dorg.osgi.test.broadcast=localhost
#-Dorg.osgi.test.seewait=false
#-Dorg.osgi.test.testcase.timeout=60000
-Dorg.osgi.test.watchdog=120
-Dscr.bundle.name=Declarative-Services

-install file:../../osgi.ri-tck/osgi.core.impls.jar
-install file:../../osgi.ri-tck/testcond.jar
-install file:../../osgi.ri-tck/osgi.target.jar

# Load the default configuration
-xargs init.xargs

-stop httproot/httproot-1.0.0.jar

-Dorg.knopflerfish.prefs.dir=fwdir/prefsdir
-istart  prefs/prefs_all-1.1.0.jar
-istart  event/event_all-1.0.0.jar
-istart  metatype/metatype-1.0.0.jar
-istart  measurement/measurement-1.0.0.jar
-istart  io/io_all-1.0.0.jar
-istart  position/position-1.0.0.jar
-istart  provisioning/provisioning_api-1.0.0.jar
-istart  upnp/upnp_api-1.0.0.jar
-istart  wireadmin/wireadmin_api-1.0.0.jar

-start  file:../../osgi.ri-tck/osgi.target.jar


