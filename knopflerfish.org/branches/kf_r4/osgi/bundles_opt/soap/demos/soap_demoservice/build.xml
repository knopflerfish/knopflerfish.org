<?xml version="1.0"?>

<!DOCTYPE project [
  <!ENTITY bundlebuild_include  SYSTEM "../../../../../ant/bundlebuild_include.xml">
]>

<project name="soap_demoservice" default="all">

  <property name="topdir"       location="../../../.."/>


  <property name  = "bundle.build.all"  value="true"/>
  <property name  = "bundle.build.lib"  value="false"/>
  <property name  = "bundle.build.api"  value="false"/>
  <property name  = "bundle.build.impl" value="false"/>

  <property name  = "api.pattern"
            value = "org/knopflerfish/service/soap/demo1/*"/>

  <property name  = "impl.pattern"
            value = "org/knopflerfish/bundle/soap/demo1/*"/>


  <path id="bundle.compile.path">
  </path>


  <property name="bmfa.Bundle-Description"    value="SOAP demo bundle"/>
  <property name="bmfa.Bundle-Vendor"         value="Knopflerfish"/>
  <property name="bmfa.Bundle-Category"       value="example"/>
  <property name="bmfa.Bundle-Version"        value="1.0.0"/>

  &bundlebuild_include;

</project>
