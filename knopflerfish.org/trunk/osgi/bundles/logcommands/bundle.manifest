Bundle-SubversionURL: https://www.knopflerfish.org/svn/knopflerfish.org/trunk
 /osgi/bundles/logcommands/
Bundle-Activator: org.knopflerfish.bundle.logcommands.LogCommands
Bundle-Category: management
