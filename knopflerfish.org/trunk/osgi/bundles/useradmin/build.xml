<?xml version="1.0"?>

<!DOCTYPE project [
  <!ENTITY bundlebuild_include  SYSTEM "../../../ant/bundlebuild_include.xml">
]>

<project name="useradmin" default="all">

  <property name="topdir"       location="../.."/>

  <property name="api.pattern"  
            value="org/osgi/service/useradmin/**,org/knopflerfish/service/um/useradmin/*,org/knopflerfish/service/um/ipam/*"/>
  <property name="impl.pattern" 
            value="org/knopflerfish/service/um/useradmin/impl/*"/> 

  <path id="bundle.compile.path">
   <pathelement location="${topdir}/jars/log/log_api-2.0.1.jar"/>
   <pathelement location="${topdir}/jars/event/event_api-2.0.1.jar"/>
   <pathelement location="${topdir}/jars/cm/cm_api-2.0.1.jar"/>
  </path>

  &bundlebuild_include;

</project>
