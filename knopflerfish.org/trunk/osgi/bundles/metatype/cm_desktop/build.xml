<?xml version="1.0"?>

<!DOCTYPE project [
  <!ENTITY bundlebuild_include  SYSTEM "../../../../ant/bundlebuild_include.xml">
]>

<project name="cm_desktop" default="all">

  <property name="topdir"       location="../../.."/>

  <property name = "ee.check.foundation"   value = "false"/>

  <property name  = "impl.pattern"
            value = "org/knopflerfish/bundle/desktop/**"/>

<!--
  <property name = "bundle.icon" value="boing32x32.png"/>
-->

  <path id="bundle.compile.path">
   <pathelement location="${topdir}/jars/log/log_api-2.0.1.jar"/>
   <pathelement location="${topdir}/jars/cm/cm_api-2.0.1.jar"/>
   <pathelement location="${topdir}/jars/util/util-2.0.0.jar"/>
   <pathelement location="${topdir}/jars/desktop/desktop_api-2.0.1.jar"/>
   <pathelement location="${topdir}/jars/metatype/metatype-2.0.0.jar"/>
   <pathelement location="${topdir}/jars/kf_metatype/kf_metatype-2.0.1.jar"/>
  </path>
  
  &bundlebuild_include;

</project>



