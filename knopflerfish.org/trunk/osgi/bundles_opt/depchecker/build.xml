<?xml version="1.0" encoding="UTF-8"?>

<project name="depchecker" default="all">

  <dirname  property="proj.dir" file="${ant.file.depchecker}"/>
  <property name="topdir"       location="${proj.dir}/../.."/>

  <property name = "ee.check.foundation"   value = "true"/>

  <property name="impl.pattern"
            value="**"/>


  <path id="bundle.compile.path">
  </path>


  <property name="bmfa.Bundle-Description" value="Automatic Resolve Dependencies"/>
  <property name="bmfa.Bundle-Category"    value="service"/>
  <property name="bmfa.Bundle-Version"     value="1.0.0"/>

  <import file="${topdir}/../ant/bundlebuild.xml"/>

</project>
