<?xml version="1.0"?>

<metatype:metatype
        xmlns:metatype = "http://www.knopflerfish.org/XMLMetatype"
        xmlns:xsd      = "http://www.w3.org/2001/XMLSchema">

  <xsd:schema >
   <xsd:complexType 
     name      = "org.knopflerfish.util.metatype.SystemMetatypeProvider"
     maxOccurs = "1">

    <xsd:annotation>
     <xsd:documentation>Settings for the system metatype provider
     </xsd:documentation>
    </xsd:annotation>

    <xsd:complexType name = "external.metatype.urls">

     <xsd:annotation>
      <xsd:documentation>List of URLs for external Metatype XML files.
      </xsd:documentation>
     </xsd:annotation>

     <xsd:sequence array="false">
      <xsd:simpleType name = "item">
        <restriction base="xsd:string"/>
      </xsd:simpleType>
     </xsd:sequence>

    </xsd:complexType>
   </xsd:complexType> 
  </xsd:schema>

<metatype:values>
 <org.knopflerfish.util.metatype.SystemMetatypeProvider>
  <external.metatype.urls>
   <item>file:</item>
  </external.metatype.urls>
 </org.knopflerfish.util.metatype.SystemMetatypeProvider>
 
</metatype:values>

</metatype:metatype>



