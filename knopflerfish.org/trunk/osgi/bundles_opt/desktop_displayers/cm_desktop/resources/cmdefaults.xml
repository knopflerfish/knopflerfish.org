  <factory1>
   <string1>apa</string1>
   <boolean1>true</boolean1>
   <intvector1>
    <item>1</item>
    <item>2</item>
    <item>3</item>
   </intvector1>
  </factory1>
   
  <org.knopflerfish.log.config>

