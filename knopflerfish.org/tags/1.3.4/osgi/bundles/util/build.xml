<?xml version="1.0"?>

<!DOCTYPE project [
  <!ENTITY bundlebuild_include  SYSTEM "../../../ant/bundlebuild_include.xml">
]>

<project name="util" default="all">

  <property name="topdir"       location="../.."/>


  <path id="bundle.compile.path">
  </path>

  <property name = "bundle.build.lib"   value = "true"/>
  <property name = "bundle.build.api"   value = "false"/>
  <property name = "bundle.build.impl"  value = "false"/>
  <property name = "bundle.build.all"   value = "false"/>

  <property name="api.pattern"  value="org/knopflerfish/util/**"/>

  <property name="bundle.category"       value="util"/>
  <property name="bundle.description"    value="Misc utilities"/>
  <property name="bundle.vendor"         value="Knopflerfish"/>
  <property name="bundle.apivendor"      value="Knopflerfish"/>
  <property name="bundle.version"        value="1.0.0"/>
  <property name="bundle.name"           value="util"/>

  &bundlebuild_include;

</project>
